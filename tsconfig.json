    "outDir": "./dist/",
   "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "target": "es5",
    "resolveJsonModule": true,
    "lib": [
